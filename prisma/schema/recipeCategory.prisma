model RecipeCategory {
  id                   String           @id @default(uuid())
  name                 String
  url                  String
  parentId             String?
  parent               RecipeCategory?  @relation("Parent", fields: [parentId], references: [id])
  parentRecipeCategory RecipeCategory[] @relation("Parent")
  Recipe               Recipe[]         @relation("RecipeCategory")
  keywords             String?
  description          String?
  text                 String?
  createdAt            DateTime         @default(now())
  updatedAt            DateTime?        @updatedAt
}
