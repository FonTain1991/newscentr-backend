model Ingredient {
  id                String             @id @default(uuid())
  previewId         String?
  previewAlt        String?
  previewTitle      String?
  iconId            String?
  title             String?
  description       String?
  parentId          String?
  parent            Ingredient[]       @relation("Parent")
  parentIngredient  Ingredient[]       @relation("Parent")
  recipeIngredients RecipeIngredient[]
}
